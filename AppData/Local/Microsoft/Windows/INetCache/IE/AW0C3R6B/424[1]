		function SetPOWERSUITE(var1, var2, var3, var4, var5, var6, var7, var8, var9, var10, var11, var12, var13, var14, var15) {

		    //if (language == "en") 
		    {
		        document.getElementById("idBottomDiv").innerHTML = document.getElementById("IDPOWERSUITE").innerHTML;
		        document.getElementById("stringRCPname").innerHTML = var1;
		        document.getElementById("stringRCPTagline").innerHTML = var2;
		        document.getElementById("stringRCPInstalled").innerHTML = var3;
		        document.getElementById("stringRCPregister").innerHTML = var4;

		        document.getElementById("stringADUname").innerHTML = var5;
		        document.getElementById("stringADUTagline").innerHTML = var6;
		        document.getElementById("stringADUInstalled").innerHTML = var7;
		        document.getElementById("stringADUregister").innerHTML = var8;

		        document.getElementById("stringASPname").innerHTML = var9;
		        document.getElementById("stringASPTagline").innerHTML = var10;
		        document.getElementById("stringASPInstalled").innerHTML = var11;
		        document.getElementById("stringASPregister").innerHTML = var12;

		        document.getElementById("stringSol").innerHTML = var13;
		        document.getElementById("stringStatus").innerHTML = var14;
		        document.getElementById("stringAction").innerHTML = var15;
		    }

		}

		function SetPOWERSUITEREGSTATE(rcp_registered, adu_registered, asp_registered) {

		    // GraySmallText
		    // OrangeSmallText
		    // BlueLinkText
		    //GreenSmallText1
		    rcp_registered = parseInt(rcp_registered);
		    adu_registered = parseInt(adu_registered);
		    asp_registered = parseInt(asp_registered);
		    if (rcp_registered == 1) {

		        document.getElementById("stringRCPInstalled").className = 'GraySmallText';
		        document.getElementById("stringRCPregister").className = 'GreenSmallText1';
		    }
		    if (adu_registered == 1) {
		        document.getElementById("stringADUInstalled").className = 'GraySmallText';
		        document.getElementById("stringADUregister").className = 'GreenSmallText1';
		    }

		    if (asp_registered == 1) {
		        document.getElementById("stringASPInstalled").className = 'GraySmallText';
		        document.getElementById("stringASPregister").className = 'GreenSmallText1';
		    }

		}
		
		
						
