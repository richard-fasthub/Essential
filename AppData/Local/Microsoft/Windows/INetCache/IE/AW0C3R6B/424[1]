		        function setlanguage(lang) {

		            language = lang;

		            if (language == "it" || language == "de" || language == "da" || language == "fr" || language == "pt" || language == "ptbr" || language == "sv" || language == "es" || language == "ja" || language == "el" || language == "ru" || language == "tr" || language == "pl" || language == "nl") {

		                languagesize = "small"
		            }
		        
		        }
		        
		        /*Include Email format*/
                function Checkformat_Email(email) {
                    
                    var validchars= /^[a-z0-9A-Z/._\-@]+$/i;
                    var emailformat = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[_-[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

                    
                    if (emailformat.test(email) && validchars.test(email)) {                        
                        var after_at_dotchar=email.substring(email.indexOf("@")+1,email.lastIndexOf("."));        
                        var firstChar=after_at_dotchar.charAt(0);
                                            
                        if(firstChar!="-"){
                            return 1;
                        }
                        else{
                            return 0;
                        }
                    }
                    else {
                        return 0;
                    }
                }
                
                /*Include Email format*/
                function Checkformat_Email(email) {
                    var validchars= /^[a-z0-9A-Z/.\-_@]+$/i;
                    var emailformat = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[_-[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

                    //var emailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
                    if (emailformat.test(email) && validchars.test(email)) {
                        return 1;
                    }
                    else {
                        return 0;
                    }
                }

