		function SetASOPROM(var1, var2, var3, var4, var5, var6, var7, var8, var9, var10, var11, var12) {

		    document.getElementById("sRecommended").innerHTML = var1;
		    document.getElementById("sSysCleaner").innerHTML = var2;
		    document.getElementById("sSecDelete").innerHTML = var3;
		    document.getElementById("sBtnDownloadNow").innerHTML = var4;
		    document.getElementById("sDiskOpt").innerHTML = var5;
		    document.getElementById("sGameOpt").innerHTML = var6;
		    document.getElementById("sDriverUpdater").innerHTML = var7;
		    document.getElementById("sMemOpt").innerHTML = var8;
		    document.getElementById("sPrivProt").innerHTML = var9;
		    document.getElementById("sDFF").innerHTML = var10;
		    document.getElementById("sUnDelete").innerHTML = var11;
		    document.getElementById("sMoreTools").innerHTML = var12;

		    if (language == "fr") {
		        document.getElementById("sBtnDownloadNow").className = "ButtonWhiteBoldASOPromTextFR";
		    }

		    document.getElementById("idBottomDiv").innerHTML = document.getElementById("ASOPROM").innerHTML;



		    //		    if (language != "el" && language != "tr" && language != "ko" && language != "pl") {

		    //		        if (language == "pt" || language == "ptbr") {
		    //		            document.getElementById("imgASOPROM").src = "homebanner_pt.jpg"
		    //		        }
		    //		        else if (language == "zhcn" || language == "zhtw") {
		    //		        document.getElementById("imgASOPROM").src = "homebanner_zh.jpg"
		    //		        }
		    //		        else {
		    //		            document.getElementById("imgASOPROM").src = "homebanner_" + language + ".jpg";
		    //		        }
		    //		    }
		}


		//////////////////////////////////////////////////////////////////////////
		//var1 ="rcpname", var2="tagline", var3="Installed", var4="registernow"
		//var5 ="ADUname", var6="tagline", var7="Installed", var8="registernow"
