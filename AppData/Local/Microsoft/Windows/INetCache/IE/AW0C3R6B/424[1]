		<script>

		    function OnWelcomeNext(ncount, nDamageLevel, strdate, ndays, Engdate, strCount, isReg, remainingdays, is_power_bundle_enabled, remdaysToCheckForAboutToExpire, idFourthPointEnabled )	
		{

		    ndays = parseInt(ndays);
		    nDamageLevel = parseInt(nDamageLevel);
		    idFourthPointEnabled = parseInt(idFourthPointEnabled);
						
						if (ndays	== -1) 
						{
								//no scan	performed
						    window.document.getElementById("idscanned").innerHTML = "";
						    window.document.getElementById("Errorcountcol").innerHTML = Engdate;

						    window.document.getElementById("idFixErrors").innerHTML = string106;
							window.document.getElementById("iddangerlevel").src='Small_level_1.gif';
		
						}
						else
						{

						    if (ndays <= 7) {
						        window.document.getElementById("idscanned").className = 'LastScanText';

						        if (languagesize == "small") {
						            if (language == "it" || language == "pt" || language == "ptbr" || language == "de" || language == "no" || language == "sv" || language == "pl") {
						                window.document.getElementById("idscanned").className = 'LastScanTextSmallpt';
						            
						            }
						            else {window.document.getElementById("idscanned").className = 'LastScanTextSmall';
						            }
						            
						            
						        }
						        
						    }
						    else {
						        if (ndays > 7) {
						            window.document.getElementById("idscanned").className = 'LastScanStatusText';

						            if (languagesize == "small") {
						                window.document.getElementById("idscanned").className = 'LastScanStatusTextSmall';
						            }
						            
						        }
						    }
								window.document.getElementById("idscanned").innerHTML	=	Engdate;
								window.document.getElementById("Errorcountcol").innerHTML = strCount;
								
								
								if (ncount > 0)	{
								    window.document.getElementById("idFixErrors").innerHTML = string103;
								}
								else {
								    window.document.getElementById("idFixErrors").innerHTML = string106;
								}

								if (ncount == 0) {
								    window.document.getElementById("iddangerlevel").src = 'Small_level_1.gif'
								    window.document.getElementById("Errorcountcol").className	=	"GreenSmallBoldText";
								    window.document.getElementById("idStatusBg").style.backgroundImage = 'url(info_box_gray.jpg)';
								    window.document.getElementById("Errorcountcol").innerHTML = string170;


								}
								else if (nDamageLevel == 2 /*ncount >	0	&& ncount	<= 10*/) {
										window.document.getElementById("iddangerlevel").src	=	'Small_level_2.gif'
										//window.document.getElementById("damagelevel").innerHTML	=	string108;
										if (language == "it" || language == "de") { 
										window.document.getElementById("Errorcountcol").className = 'RedSmallBoldTextit'; }
										else {
										window.document.getElementById("Errorcountcol").className = "RedSmallBoldText";
										}
										
										
										
										window.document.getElementById("idStatusBg").style.backgroundImage = 'url(info_box_gray.jpg)';
										
										
								}
								else if	(nDamageLevel == 4/*ncount	>	10 &&	ncount <=	50*/)	{
								window.document.getElementById("iddangerlevel").src	=	'Small_level_4.gif'
								window.document.getElementById("idStatusBg").style.backgroundImage = 'url(info_box_red.jpg)';
								if (language == "it") {
								    window.document.getElementById("Errorcountcol").className = 'RedSmallBoldTextit';
								}
								else {
								    window.document.getElementById("Errorcountcol").className = "RedSmallBoldText";
								}
												//window.document.getElementById("damagelevel").innerHTML= string107;
										}
										else {
												window.document.getElementById("iddangerlevel").src	=	'Small_level_6.gif'
												window.document.getElementById("idStatusBg").style.backgroundImage = 'url(info_box_red.jpg)';
												if (language == "it") {
												    window.document.getElementById("Errorcountcol").className = 'RedSmallBoldTextit';
												}
												else {
												    window.document.getElementById("Errorcountcol").className = "RedSmallBoldText";
												}
												//window.document.getElementById("damagelevel").innerHTML= string101;
								}


				}

				        remdaysToCheckForAboutToExpire = parseInt(remdaysToCheckForAboutToExpire);
                        is_power_bundle_enabled = parseInt(is_power_bundle_enabled);
                        /*
                         *	
                         if (language == "en" && is_power_bundle_enabled == 1) {
                            document.getElementById("idBottomDiv").innerHTML = document.getElementById("IDPOWERSUITE").innerHTML;                            
						}
						else {
						    document.getElementById("IDPOWERSUITE").style.visibility = "hidden";
						    document.getElementById("IDPOWERSUITE").style.display = "none";*/
						    isReg = parseInt(isReg);
						    if (isReg == 0) {
						        document.getElementById("idBottomDiv").innerHTML = document.getElementById("idBottomDiv1").innerHTML;

						        if (idFourthPointEnabled == 0) {
						            document.getElementById("idFourthPoint").style.visibility = "hidden";
						            document.getElementById("idFourthPoint").style.display = "none";
						        }
						        else {
						            document.getElementById("idFourthPoint").style.visibility = "visible";
						            document.getElementById("idFourthPoint").style.display = "block";
						            document.getElementById("string113").style.fontSize = "16px";
						            document.getElementById("string114").style.fontSize = "16px";
						            document.getElementById("string115").style.fontSize = "16px";
						            document.getElementById("IDS_WORKS_ON").style.fontSize = "16px";
						        }
						        
						    }
						    else {
						        remainingdays = parseInt(remainingdays);
						        if (remainingdays == 0) {
						            document.getElementById("idBottomDiv").innerHTML = document.getElementById("idBottomDiv6").innerHTML;
						        }
						        else if (remainingdays <= remdaysToCheckForAboutToExpire/*9*/) {

						            string117 = string117.replace("%d", remainingdays);

						            if (remainingdays == 1) {
						                string117 = string117.replace("days", "day");
						            }


						            document.getElementById("iddaysleft").innerHTML = string117;
						            document.getElementById("idBottomDiv").innerHTML = document.getElementById("idBottomDiv2").innerHTML;
						        }
						        else {
