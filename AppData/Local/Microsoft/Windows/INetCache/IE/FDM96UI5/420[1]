    window.document.getElementById("STR_IDS_ACDLG_UNCLUT_REG").innerHTML = var7;
    window.document.getElementById("STR_IDS_ACDLG_FEWER_ERR").innerHTML = var8;
    window.document.getElementById("STR_IDS_ACDLG_SAFE").innerHTML = var9;

}


function setPromoteADU(varADUPromotion)
{
    var1 = parseInt(varADUPromotion);
    if (var1 == 1) {
        document.getElementById("idmaindiv").innerHTML = document.getElementById("adupromotion").innerHTML;
    }
    else {
        document.getElementById("idmaindiv").innerHTML = document.getElementById("noadupromotion").innerHTML;
    }

}

function setPromoteASO(varASOPromotion) {
    var1 = parseInt(varASOPromotion);
    if (var1 == 1) {
        document.getElementById("idmaindiv").innerHTML = document.getElementById("asopromotion").innerHTML;
    }
    else {
        document.getElementById("idmaindiv").innerHTML = document.getElementById("noadupromotion").innerHTML;
    }

}


