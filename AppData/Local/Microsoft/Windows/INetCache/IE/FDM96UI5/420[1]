function LoadStrings(var1, var2, var3, var4, var5, var6, var7, var8, var9) {
    window.document.getElementById("STR_IDS_CONGS").innerHTML = var1; 
    window.document.getElementById("STR_IDS_RECOMMN").innerHTML = var2; 
    window.document.getElementById("STR_IDS_ADU").innerHTML = var3;
    window.document.getElementById("STR_IDS_LIST").innerHTML = var4;

