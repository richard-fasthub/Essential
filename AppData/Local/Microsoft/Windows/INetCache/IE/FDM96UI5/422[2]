                var productname = "";
                function ProductName(productname) {
                    productname = productname;
                   
                    if (productname == "DLDU") {
                        
                        if (window.document.getElementById("btnPurchaseNow")) {
                            window.document.getElementById("btnPurchaseNow").style.height = "33px";
                        } 

                    }
                    else {
                   
                        if (window.document.getElementById("btnPurchaseNow")) {
                            window.document.getElementById("btnPurchaseNow").style.height = "28px";
                        }
                        

                    }
                }
				     
