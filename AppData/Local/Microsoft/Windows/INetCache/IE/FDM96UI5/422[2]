						 function	changebg(myvar,	whattodo)	{

								 var i = parseInt(whattodo);
								 var d = myvar.style

								 if	(i ==	0) {
										 d.backgroundImage = 'url('	+	pictures[0].src	+	')';
								 }
								 else	if (i	== 1)	{
										 d.backgroundImage = 'url('	+	pictures[1].src	+	')';
								 }
								 else	if (i	== 2)	{
										 d.backgroundImage = 'url('	+	pictures[2].src	+	')';
								 }


						 }

						 function	changebgupgrade(myvar, whattodo) {

								 var i = parseInt(whattodo);
								 var d = myvar.style

								 if	(i ==	0) {
										 d.backgroundImage = 'url('	+	pictures[3].src	+	')';
								 }
								 else	if (i	== 1)	{
										 d.backgroundImage = 'url('	+	pictures[4].src	+	')';
								 }
								 else	if (i	== 2)	{
										 d.backgroundImage = 'url('	+	pictures[5].src	+	')';
								 }


					}
					function ChangeImage(idbtn, imgLeft, imgRight, imgMiddle) {
					    document.getElementById(idbtn + "Left").src = imgLeft + ".jpg";
					    document.getElementById(idbtn + "Right").src = imgRight + ".jpg";
					    document.getElementById(idbtn + "Middle").style.backgroundImage = "url(" + pictures[imgMiddle].src + ")";
					}

					function Change30daysMBG_logo_whiteBGImg() {
					    var AppendLangCode = "_" + language;
					    if (language == "en") {
					        AppendLangCode = '';
					    }
					    
					    var ImageName = "30daysMBG_logo_whiteBG" + AppendLangCode + ".jpg";
					    document.getElementById("30daysMBG_logo_whiteBGImg").src = ImageName;
					}
		
