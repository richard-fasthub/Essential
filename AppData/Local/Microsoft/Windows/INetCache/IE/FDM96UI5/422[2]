						 function	settext(ncount, nDamageLevel) {				     
    
                                
						        nDamageLevel = parseInt(nDamageLevel);
								 ncount	=	parseInt(ncount);
								 window.document.getElementById('idError').innerHTML = ncount;


								 if (nDamageLevel == 2/*ncount	>= 0 &&	ncount <=	10*/) {
										 window.document.getElementById("iddangerlevel").src = 'Small_level_2.gif'
										 window.document.getElementById("idsystemhealth").innerHTML = string108;
										 window.document.getElementById("idsystemhealth").className= 'GreenText';
										 

								 }
								 else if (nDamageLevel == 4/*ncount > 10	&& ncount	<= 50*/) {

										 window.document.getElementById("iddangerlevel").src = 'Small_level_4.gif'
										 window.document.getElementById("idsystemhealth").innerHTML = string107;
										 window.document.getElementById("idsystemhealth").className = 'YellowText';

								 }
								 else	{
										 window.document.getElementById("iddangerlevel").src = 'Small_level_6.gif'
										 window.document.getElementById("idsystemhealth").innerHTML = string101;
										 window.document.getElementById("idsystemhealth").className = 'RedText';
								 }

					}


