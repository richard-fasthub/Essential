        function mouseover(idbtn, btn_h_left, btn_h_right, btn_h_middle) {
            //alert(btn_h_left);
            document.getElementById(idbtn + "Left").src = btn_h_left;
            document.getElementById(idbtn + "Right").src = btn_h_right;
            document.getElementById(idbtn + "Middle").style.backgroundImage = "url(" + btn_h_middle + ")";
        }

        function mouseout(idbtn, btn_n_left, btn_n_right, btn_n_middle) {
            document.getElementById(idbtn + "Left").src = btn_n_left;
            document.getElementById(idbtn + "Right").src = btn_n_right;
            document.getElementById(idbtn + "Middle").style.backgroundImage = "url(" + btn_n_middle + ")";
        }
        function MouseDown(idbtn, btn_d_left, btn_d_right, btn_n_middle) {
            document.getElementById(idbtn + "Left").src = btn_d_left;
            document.getElementById(idbtn + "Right").src = btn_d_right;
            document.getElementById(idbtn + "Middle").style.backgroundImage = "url(" + btn_n_middle + ")";
        }
        
        
         var language = "en";
		 var languagesize = "normal";


		    function setlanguage(lang) {

		        language = lang;

		        if (language == "it" || language == "de" || language == "da" || language == "fr" || language == "pt" || language == "ptbr" || language == "sv" || language == "es" || language == "ja" || language == "el" || language == "ru" || language == "nl") {

		            languagesize = "small"
		        }

		    }

		    var winmajor = "6";
		    function setosid(osid) {
		        winmajor = parseInt(osid);
		    }
		    

        function settext(varCondition,          // state of application driver scan
                        idText1,				//"<b>Last Scan Result:</b>";
                        idText2,				//"Last Driver Scan:";
                        idText3,				//"Today";
                        idText4,				//"Oldest outdated driver:";
                        idText5,				//"4 years old";
                        idText6,				//"Total outdated drivers found:";
                        idText7,				//"22 drivers";
                        btnLastScanResults,	    //"View last scan results";
                        idText9 ,				//"22";
                        idText10,				//"OUT OF DATE";
                        idText11,				//"DRIVERS FOUND";
                        btnUpdateNow,			//"Update Now";
                        idText13,				//"<b>Categories covered by each scan:</b>";
                        idText14,				//"Disk drives";
                        idText15,				//"Display adapters";
                        idText16,				//"DVD/CD-ROM drives";
                        idText17,				//"IDE ATA/ATAPI controllers";
                        idText18,				//"Monitors";
                        idText19,				//"Network adapters";
                        idText20,				//"Processors";
                        idText21,				//"Other devices";
                        idText22,				//"Sound, video and game controllers";
                        idText23,				//"System devices";
                        idText24,				//"SCSI/RAID";
                        idText25,				//"Universal Serial Bus controllers";
                        idText27,				//"DRIVER STATUS";
                        idText28,				//"UNCHECKED";
                        btnStartScanNow,		//"Start Scan";
                        idText30,				//"DRIVER STATUS";
                        idText31,				//"UPDATED";
                        btnScanNow,				//"Scan Now";
                        varUpdatedTxt,          // Updated
                        varAllDriversAreUpToDate, // All Driver are up to date 
                        varLastScanText,             // Last scan
                        ndays,
                        varOsId,
                        varDriverAgeText,
                        string165, 
                        string166,
                        string167,                        
                        nDamageLevel,
                        varNoScanPerformedYet 
                        
                        ) {

            ndays = parseInt(ndays);
            varOsId = parseInt(varOsId);
            ncount = parseInt(idText9);
            nDamageLevel = parseInt(nDamageLevel);
            
            var nState = parseInt(varCondition);

                

            document.getElementById("idText1").innerHTML = "" + idText1 + "";        //"<b>Last Scan Result:</b>";
            document.getElementById("idText4").innerHTML = idText4;                         //"Oldest outdated driver:";
            document.getElementById("idText5").innerHTML = idText5;                         //"4 years old";
            document.getElementById("idDriverAgeText").innerHTML = varDriverAgeText;                         //"4 years old";
            document.getElementById("string165").innerHTML = string165;                         //"4 years old";
            document.getElementById("string166").innerHTML = string166;                         //"4 years old";
            document.getElementById("string167").innerHTML = string167;                         //"4 years old";
            document.getElementById("idHeadingUpdatedText").innerHTML = idText3;                         //"4 years old";                        
            document.getElementById("btnLastScanResults").innerHTML = btnLastScanResults;    //"View last scan results";
            document.getElementById("idText9").innerHTML = idText9;                          //"22";
            document.getElementById("idText10").innerHTML = idText10;                        //"OUT OF DATE";
            document.getElementById("idText11").innerHTML = idText11;                        //"DRIVERS FOUND";
            document.getElementById("btnUpdateNow").innerHTML = btnUpdateNow;                //"Update Now";
            document.getElementById("idText13").innerHTML = "" + idText13 + "";              //"<b>Categories covered by each scan:</b>";
            document.getElementById("idText14").innerHTML = idText14;                        //"Disk drives";
            document.getElementById("idText15").innerHTML = idText15;                        //"Display adapters";
            document.getElementById("idText16").innerHTML = idText16;                        //"DVD/CD-ROM drives";
            document.getElementById("idText17").innerHTML = idText17;                        //"IDE ATA/ATAPI controllers";
            document.getElementById("idText18").innerHTML = idText18;                        //"Monitors";
            document.getElementById("idText19").innerHTML = idText19;                        //"Network adapters";
            document.getElementById("idText20").innerHTML = idText20;                        //"Processors";
            document.getElementById("idText21").innerHTML = idText21;                        //"Other devices";
            document.getElementById("idText22").innerHTML = idText22;                        //"Sound, video and game controllers";
            document.getElementById("idText23").innerHTML = idText23;                        //"System devices";
            document.getElementById("idText24").innerHTML = idText24;                        //"SCSI/RAID";
            document.getElementById("idText25").innerHTML = idText25;                        //"Universal Serial Bus controllers";            
            document.getElementById("idText28").innerHTML = "<b>" + idText28 + "</b>";                        //"UNCHECKED";
            document.getElementById("btnStartScanNow").innerHTML = btnStartScanNow;                 //"Start Scan";
            if (language == "nl") {
                
                if (idText27.length > 14) {                    
                    var lenchar = 14;
                    var strSplit1 = idText27.substring(0, lenchar);
                    var strSplit2 = idText27.substring(lenchar);                    
                    document.getElementById("idText27").innerHTML = "<b>" + strSplit1 + "<br>" + strSplit2 + "</b>";       //"DRIVER STATUS";
                }
            }
            else {
                
                document.getElementById("idText27").innerHTML = "<b>" + idText27 + "</b>";       //"DRIVER STATUS";
                
            }

            document.getElementById("idText30").innerHTML = idText30;                        //"DRIVER STATUS";
            document.getElementById("idText31").innerHTML = idText31;                        //"UPDATED";
            document.getElementById("btnScanNow").innerHTML = btnScanNow;                     //"Scan Now";
            //document.getElementById("idHeadingUpdatedText").innerHTML = "";             // Updated

            if (language == "en") {
                document.getElementById("idBlueMiddle").width = "94px";    
            }
            
            
            document.getElementById("idHeadingUpdatedText").innerHTML = idText3;                         //"4 years old";
            document.getElementById("idHeadingUpdatedText").className = "DriversFoundText";
            document.getElementById("idHeadingUpdatedText").style.marginTop = "10px";

            


            /* make it No Scan Performed Yet */
            if (ndays == -1) {
                nState = 0;
            }

            //nState = 0;
            /* No Scan Performed Yet */
            if (nState == 0) {
                document.getElementById("divStatus1").style.visibility = "visible";
                document.getElementById("divStatus1").style.display = "block";

                document.getElementById("divStatus2").style.visibility = "hidden";
                document.getElementById("divStatus2").style.display = "none";

                document.getElementById("divStatus3").style.visibility = "hidden";
                document.getElementById("divStatus3").style.display = "none";                             
    

                document.getElementById("idLastScanSummary").style.visibility = "visible";
                document.getElementById("idLastScanSummary").style.display = "block";

                document.getElementById("idDriversUpToDateTbl").style.visibility = "hidden";
                document.getElementById("idDriversUpToDateTbl").style.display = "none";

                document.getElementById("idHeadingUpdatedText").innerHTML = varNoScanPerformedYet;
                document.getElementById("idHeadingUpdatedText").className = 'RedSmallBoldText';
                
                window.document.getElementById("iddangerlevel").src = 'BIGBARLEVEL_1.jpg';
                window.document.getElementById("idText5").innerHTML = /*"<br>" + */varNoScanPerformedYet;
                //window.document.getElementById("idText5").style.paddingLeft = "20px"
                window.document.getElementById("idText5").className = 'RedSmallBoldText';

               // document.getElementById("idbtnStartScanNowUnchecked").style.textAlign = "left";
                
                
                document.getElementById("idbtnLastScanResults").style.visibility = "hidden";
                document.getElementById("idbtnLastScanResults").style.display = "none";

                document.getElementById("idbtnStartScanNowUnchecked").style.visibility = "visible";
                document.getElementById("idbtnStartScanNowUnchecked").style.display = "block";
                document.getElementById("btnStartScanNowUnchecked").innerHTML = btnStartScanNow;
                
                /*
                document.getElementById("idText5").innerHTML = idText3;
                document.getElementById("idText7").innerHTML = idText3;                         //"22 drivers";    
                */

            }
            /* Outdated drivers found */
            else if (nState == 1) {
                document.getElementById("divStatus2").style.visibility = "visible";
                document.getElementById("divStatus2").style.display = "block";

                document.getElementById("divStatus1").style.visibility = "hidden";
                document.getElementById("divStatus1").style.display = "none";

                document.getElementById("divStatus3").style.visibility = "hidden";
                document.getElementById("divStatus3").style.display = "none";

                document.getElementById("btnStartScanNowUnchecked").style.visibility = "hidden";
                document.getElementById("btnStartScanNowUnchecked").style.display = "none"; 

                /*                	
                document.getElementById("idText3").className = "RedMediumText";                
                document.getElementById("idText7").className = "RedMediumText";
                document.getElementById("idText5").className = "RedMediumText";
                */
                

                document.getElementById("idLastScanSummary").style.visibility = "visible";
                document.getElementById("idLastScanSummary").style.display = "block";


                document.getElementById("idDriversUpToDateTbl").style.visibility = "hidden";
                document.getElementById("idDriversUpToDateTbl").style.display = "none";

                
                document.getElementById("idbtnLastScanResults").style.visibility = "visible";
                document.getElementById("idbtnLastScanResults").style.display = "block";
                
                

//                if (varOsId < 6) {
//                    document.getElementById("idNotepadBtn1").style.marginTop = "0px";
//                    if (language == "ptbr" || language == "es" || language == "de") {
//                        document.getElementById("idNotepadBtn1").style.marginLeft = "10px";
//                    }
//                }
//                else {
//                    if (language == "es" || language == "de") {
//                        document.getElementById("idNotepadBtn1").style.marginLeft = "10px";
//                    }
//                }

                
                if (nDamageLevel == 2) {
                    window.document.getElementById("iddangerlevel").src = 'BIGBARLEVEL_2.jpg';                    
                    window.document.getElementById("idText5").className = 'BlackSmallBoldText';

                }
                else if (nDamageLevel == 4) {
                    window.document.getElementById("iddangerlevel").src = 'BIGBARLEVEL_4.jpg';                    
                    window.document.getElementById("idText5").className = 'RedSmallBoldText';

                }
                else {
                    window.document.getElementById("iddangerlevel").src = 'BIGBARLEVEL_6.jpg';                    
                    window.document.getElementById("idText5").className = 'RedSmallBoldText';
                }  
            }
            /* perfect */
            else if (nState == 2) {             
            
                document.getElementById("divStatus3").style.visibility = "visible";
                document.getElementById("divStatus3").style.display = "block";

                document.getElementById("divStatus1").style.visibility = "hidden";
                document.getElementById("divStatus1").style.display = "none";

                document.getElementById("divStatus2").style.visibility = "hidden";
                document.getElementById("divStatus2").style.display = "none";

//                document.getElementById("idText3").className = "GreenMediumText";
//                document.getElementById("idText5").className = "GreenMediumText";
//                document.getElementById("idText7").className = "GreenMediumText";

                document.getElementById("idLastScanSummary").style.visibility = "hidden";
                document.getElementById("idLastScanSummary").style.display = "none";

                document.getElementById("idDriversUpToDateTbl").style.visibility = "visible";
                document.getElementById("idDriversUpToDateTbl").style.display = "block";


                document.getElementById("idHeadingUpdatedText").innerHTML = varUpdatedTxt;             // Updated
                document.getElementById("idHeadingUpdatedText").className = "GreenHeadingTextUpdated";
                document.getElementById("idHeadingUpdatedText").style.marginTop = "8px";
                document.getElementById("idUptodateText").innerHTML = varAllDriversAreUpToDate;                   // All driver are up to date
                document.getElementById("idLastScanText").innerHTML = varLastScanText;                   // Last Scan:
                document.getElementById("idLastScanDay").innerHTML = idText3;                    // Today
                            
            }


            if (language == "en") {
                document.getElementById("idText28").style.fontSize = "16px";                
            }
            else if (languagesize == "small") {                
                document.getElementById("idText28").style.fontSize = "11px";
                document.getElementById("idText11").style.fontSize = "11px";                
                
                if (language == "ru") {                    
                    document.getElementById("idDriverStatusDiv").style.marginTop = "20px";
                }
                else if (language == "ptbr") {
                    ;
                    //document.getElementById("idNotepadBtn").style.marginLeft = "10px";
                }                
            }            
            else {
                if (language == "zhcn") {
                    ;
                    //document.getElementById("idNotepadBtn").style.marginLeft = "25px";
                }
                document.getElementById("idText28").style.fontSize = "14px";
            }

            //if (language == "nl") {
                if (winmajor < 6) {
                    document.getElementById("idOutOfDateText").style.marginTop = "10px";
                  }
                
            //}
            
           
        }                        
